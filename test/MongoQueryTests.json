  {
    "query": "select `Address.City` as City ,`First Name` as Name from `customers` where `id`>1 limit 10 offset 5",
    "output": {
      "collection": "customers",
      "limit": 10,
      "skip": 5,
      "projection": {
        "City": "$Address.City",
        "Name": "$First Name"
      },
      "query": {
        "id": { "$gt": 1 }

      }
    }
  },
  {
    "query": "select `First Name` as Name,first(`Rentals`) as Rental from `customers` where `id`>1 limit 10 offset 5",
    "output": {
      "collection": "customers",
      "limit": 10,
      "skip": 5,
      "projection": {
        "Rental": {
          "$first": "$Rentals"
        },
        "Name": "$First Name"
      },
      "query": {
        "id": { "$gt": 1 }

      }
    }
  },
  {
    "query": "select subtract(convert('1','int'),abs(`Replacement Cost`)) as d,Title from `films`",
    "output": {
      "collection": "films",
      "limit": 100,
      "projection": {
        "Title": 1,
        "d": {
          "$subtract": [
            {
              "$convert": {
                "input": {
                  "$literal": "1"
                },
                "to": "int"
              }
            },
            {
              "$abs": "$Replacement Cost"
            }
          ]
        }
      }
    }
  },
  {
    "query": "SELECT * FROM customers WHERE `First Name` like '%bc%'",
    "output": {
      "limit": 100,
      "collection": "customers",
      "query": { "First Name": { "$regex": "bc", "$options": "i" } }}
  },
  {
    "query": "SELECT abs(id + arraySize(Rentals) + 1) as testComplex FROM customers WHERE `First Name` like '%bc%'",
    "output": {
      "limit": 100,
      "collection": "customers",
      "query": { "First Name": { "$regex": "bc", "$options": "i" } }},
      "projection": {
        "testComplex": {
          "$abs": {
            "$add": [
              {
                "$add": [
                  "$id",
                  {
                    "$size": "$Rentals"
                  }
                ]
              },
              1
            ]
          }
        }
      }
  },
  {
    "query": "SELECT *,convert(id,'int') as idConv FROM customers",
    "output": {
      "limit": 100,
      "collection": "customers",
      "query": { "First Name": { "$regex": "bc", "$options": "i" } }}
  },
  {
    "query": "SELECT ceiling(id + arraySize(Rentals) + 1) as testComplex FROM customers",
    "output": {
      "limit": 100,
      "collection": "customers",
      "query": { "First Name": { "$regex": "bc", "$options": "i" } }},
    "projection": {
      "$ceil": {
        "$add": [
          {
            "$add": [
              "$id",
              {
                "$size": "$Rentals"
              }
            ]
          },
          1
        ]
      }
    }
  }


