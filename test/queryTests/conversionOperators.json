  {
    "name": "Convert:CONVERT",
    "query": "select SUBTRACT(CONVERT('1','int'),ABS(`Replacement Cost`)) as d,Title from `films`",
    "output": {
      "limit": 100,
      "collection": "films",
      "projection": {
        "d": {
          "$subtract": [
            {
              "$convert": {
                "input": {
                  "$literal": "1"
                },
                "to": "int"
              }
            },
            {
              "$abs": "$Replacement Cost"
            }
          ]
        },
        "Title": "$Title"
      }
    }
  },
