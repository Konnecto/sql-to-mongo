    "query": "select id,Title,Rating from `films` where abs(id)=1",
    "type": "aggregate",
    "output": {
      "collections": [
        "films"
      ],
      "pipeline": [
        {
          "$match": {
            "$expr": {
              "$eq": [
                {
                  "$abs": "$id"
