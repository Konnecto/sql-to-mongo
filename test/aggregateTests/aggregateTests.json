  {
    "query": "select id,Title,Rating from `films` where sum_Array((select id as total from customers),'total') =1",
    "type": "aggregate",
    "output": {
      "pipeline": [
        {
          "$match": {
            "$expr": {
              "$eq": [
                {
                  "$reduce": {
                    "input": {
                      "$map": {
                        "input": "$customers",
                        "in": {
                          "total": "$$this.id"
                        }
                      }
                    },
                    "initialValue": 0,
                    "in": {
                      "$sum": [
                        "$$value",
                        "$$this.total"
                      ]
                    }
                  }
                },
                1
              ]
            }
          }
        },
        {
          "$project": {
            "id": "$id",
            "Title": "$Title",
            "Rating": "$Rating"
          }
        }
