  {
    "query": "select id,Title,Rating from `films` where abs(id)=1 or abs(id)=2",
    "type": "aggregate",
    "output": {
      "collections": [
        "films"
      ],
      "pipeline": [
        {
          "$match": {
            "$or": [
              {
                "$expr": {
                  "$eq": [
                    {
                      "$abs": "$id"
                    },
                    1
                  ]
                }
              },
              {
                "$expr": {
                  "$eq": [
                    {
                      "$abs": "$id"
                    },
                    2
                  ]
                }
              }
            ]
          }
        },
        {
          "$project": {
            "id": "$id",
            "Title": "$Title",
            "Rating": "$Rating"
          }
        }
      ]
    }
