  {
    "name": "Select sum case with values only",
    "query": "select sum(case when id <50 then 1 when id >= 50 then -1 else 0 end) as sumCase,`Address.City` as City from `customers` group by `Address.City`",
    "type": "aggregate",
    "output": {
      "pipeline": [
        {
          "$group": {
            "_id": {
              "City": "$Address.City"
            },
            "sumCase": {
              "$sum": {
                "$switch": {
                  "branches": [
                    {
                      "case": {
                        "$lt": [
                          "$id",
                          50
                        ]
                      },
                      "then": 1
                    },
                    {
                      "case": {
                        "$gte": [
                          "$id",
                          50
                        ]
                      },
                      "then": {
                        "$multiply": [
                          -1,
                          {
                            "$literal": 1
                          }
                        ]
                      }
                    }
                  ],
                  "default": {
                    "$literal": 0
                  }
                }
              }
            }
          }
        },
        {
          "$project": {
            "City": "$_id.City",
            "_id": 0,
            "sumCase": "$sumCase"
          }
        }
      ],
      "collections": [
        "customers"
      ]
    }
  },
