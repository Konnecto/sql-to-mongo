[{
  "query": "select t.CustomerID,t.Name,t.Rental  from (select id as CustomerID,`First Name` as Name,unwind(Rentals) as Rental from customers) as t",
  "type": "aggregate",
  "output": {
    "pipeline": [
      {
        "$project": {
          "CustomerID": "$id",
          "Name": "$First Name",
          "Rental": "$Rentals"
        }
      },
      {
        "$unwind": "$Rental"
      },
      {
        "$project": {
          "t": "$$ROOT"
        }
      },
      {
        "$project": {
          "CustomerID": "$t.CustomerID",
          "Name": "$t.Name",
          "Rental": "$t.Rental"
        }
      }
    ],
    "collections": [
      "customers"
    ]
  }
},
{
  "query": "select MERGE_OBJECTS((select t.CustomerID,t.Name),t.Rental) as `$$ROOT` from (select id as CustomerID,`First Name` as Name,unwind(Rentals) as Rental from customers) as t",
  "type": "aggregate",
  "output": {
    "pipeline": [
      {
        "$project": {
          "CustomerID": "$id",
          "Name": "$First Name",
          "Rental": "$Rentals"
        }
      },
      {
        "$unwind": "$Rental"
      },
      {
        "$project": {
          "t": "$$ROOT"
        }
      },
      {
        "$replaceRoot": {
          "newRoot": {
            "$mergeObjects": [
              {
                "$arrayToObject": {
                  "$concatArrays": [
                    {
                      "$objectToArray": {
                        "CustomerID": "$t.CustomerID",
                        "Name": "$t.Name"
                      }
                    }
                  ]
                }
              },
              "$t.Rental"
            ]
          }
        }
      }
    ],
    "collections": [
      "customers"
    ]
  }
}
]